{
  "name": "receitas-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "app": "ng s --live-reload false",
    "json:server": "json-server --watch db.json",
    "start:app:server": "concurrently --kill-others \"npm run json:server\" \"npm run app\"",
    "start:node:server": "concurrently --kill-others \"npm run json:server\" \"npm start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.4",
    "@angular/cdk": "~13.3.4",
